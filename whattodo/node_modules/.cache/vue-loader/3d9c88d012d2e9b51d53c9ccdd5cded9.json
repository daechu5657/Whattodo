 let result = this.basicframe.contents.filter(function (a) {\r\n        return a.month == '4';\r\n      });\r\n      console.log(result);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\ndiv {\r\n  box-sizing: border-box;\r\n}\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.login-page {\r\n  position: absolute;\r\n  width: 1024px;\r\n  height: 100vh;\r\n  background-color: rgb(53, 74, 28);\r\n  z-index: 1000;\r\n}\r\n</style>\r\n"]}]}