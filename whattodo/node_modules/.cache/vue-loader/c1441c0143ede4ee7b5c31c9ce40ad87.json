    this.index++;\r\n      } else if (this.index == 2) {\r\n        this.텍스트.push({ value: '' });\r\n        this.index++;\r\n      }\r\n    },\r\n    save() {\r\n      if (this.텍스트.length < 1) {\r\n        alert('내용입력해쥬');\r\n      }\r\n      abc.index = this.데이터.index;\r\n      abc.title = this.데이터.title;\r\n      abc.time = this.데이터.time;\r\n      abc.order = this.데이터.order;\r\n      abc.text = this.텍스트;\r\n      console.log(abc);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\ndiv {\r\n  box-sizing: border-box;\r\n}\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.modalwrap {\r\n  position: absolute;\r\n  width: 924px;\r\n  height: 80vh;\r\n  background-color: rgba(147, 70, 70, 0.9);\r\n  z-index: 100;\r\n}\r\n.modal-top {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 10vh;\r\n}\r\n.modal-content {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 60vh;\r\n}\r\n.modal-bottom {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 10vh;\r\n}\r\n</style>\r\n"]}]}