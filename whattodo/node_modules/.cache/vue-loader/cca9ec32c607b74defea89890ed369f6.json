n              class=\"testid\"\r\n              v-model=\"loginframe._id\"\r\n              placeholder=\"Id\"\r\n            />\r\n\r\n            <input\r\n              type=\"password\"\r\n              class=\"testpwd\"\r\n              v-model=\"loginframe.pwd\"\r\n              placeholder=\"Pwd\"\r\n            />\r\n\r\n            <input\r\n              type=\"password\"\r\n              class=\"testpwdr\"\r\n              v-model=\"pwdr\"\r\n              placeholder=\"Pwd Check\"\r\n            />\r\n          </div>\r\n          <div class=\"login-bottom-contents-middle-img\">\r\n            <div\r\n              class=\"middleimg0\"\r\n              :style=\"{ backgroundImage: `url(${loginframe.img0})` }\"\r\n            ></div>\r\n            <div\r\n              class=\"middleimg1\"\r\n              :style=\"{ backgroundImage: `url(${loginframe.img1})` }\"\r\n            ></div>\r\n          </div>\r\n          <div class=\"login-bottom-contents-middle\">\r\n            <label for=\"profileimg1\">+</label>\r\n            <input\r\n              @change=\"upload0\"\r\n              type=\"file\"\r\n              class=\"profileimg1\"\r\n              id=\"profileimg1\"\r\n            />\r\n            <label for=\"profileimg2\">+</label>\r\n            <input\r\n              @change=\"upload1\"\r\n              type=\"file\"\r\n              class=\"profileimg2\"\r\n              id=\"profileimg2\"\r\n            />\r\n          </div>\r\n          <div class=\"login-bottom-contents-bottom\">\r\n            <input\r\n              type=\"text\"\r\n              class=\"profilename1\"\r\n              v-model=\"loginframe.name0\"\r\n              placeholder=\"Profile 1 Name\"\r\n            />\r\n\r\n            <input\r\n              type=\"text\"\r\n              class=\"profilename2\"\r\n              v-model=\"loginframe.name1\"\r\n              placeholder=\"Profile 2 Name\"\r\n            />\r\n          </div>\r\n          <p>&nbsp;{{ Noti