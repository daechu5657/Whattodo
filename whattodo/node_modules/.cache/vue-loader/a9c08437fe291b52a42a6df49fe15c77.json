-------------------------</p>\r\n    <input type=\"text\" class=\"testid\" v-model=\"loginframe._id\" />\r\n    <input type=\"text\" class=\"testpwd\" v-model=\"loginframe.pwd\" />\r\n    <input type=\"text\" class=\"testpwdr\" v-model=\"pwdr\" />\r\n    <input type=\"text\" class=\"profilename\" v-model=\"loginframe.name\" />\r\n    <button @click=\"sign()\">가입</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport basicframe from './modal.json';\r\nimport loginframe from './login.json';\r\nlet today = new Date();\r\nlet m = today.getMonth() + 1;\r\nlet d = today.getDate();\r\n\r\nexport default {\r\n  emits: ['userdata'],\r\n  data() {\r\n    return {\r\n      logindata: { _id: '', pwd: '' },\r\n      pwdr: '',\r\n      basicframe: basicframe,\r\n      loginframe: loginframe,\r\n      onoff: 'on',\r\n    };\r\n  },\r\n  methods: {\r\n    sign() {\r\n      if (this.loginframe.pwd == this.pwdr) {\r\n        basicframe._id = loginframe._id;\r\n        basicframe.profile[0].profilename = loginframe.name;\r\n        basicframe.profile[0].contents[0].month = m;\r\n        basicframe.profile[0].contents[0].day = d;\r\n        axios.post('/MakeUserData', this.loginframe).then(response => {\r\n          console.log(response);\r\n        });\r\n        axios.post('/MakeUserFirstProfile', this.basicframe).then(response => {\r\n          console.log(response);\r\n        });\r\n        console.log(this.basicframe);\r\n        console.log(this.loginframe);\r\n      } else {\r\n        alert('비밀번호 틀려유');\r\n      }\r\n    },\r\n    login() {\r\n      axios.post('/Login', this.logindata).then(response => {\r\n        console.log(response);\r\n        if (response.data == '잘못됨') {\r\n          alert('일치안해유');\r\n        } else {\r\n          this.$emit('getid', response);\r\n          this.onoff = 'off';\r\n        }\r\n\r\n        // if (response.data == 'off') {\r\n        //   this.$emit('getid', this.logindata);\r\n        //   this.onoff = response.data;\r\n        // } else {\r\n        //   alert('일치안해유');\r\n        // }\r\n        // 해당 회원의 컨텐츠 값이 res 이고 이값을 app.vue에 props하자\r\n        // app.vue에서의 프로필선택값을 통하여 해당배열값을 선택하여 다른컴포넌트에 props\r\n        // 근데 프로필을 새로 만든경우나 바꾸는 경우엔 다른컴포넌트에 어떻게 뿌리지?\r\n        // 어차피 app.vue에서 계속 받아야하니까 로그인은 로그인만하고\r\n        // app.vue 가 나타날때(로그인창이 사라질때) , 프로필값이 바뀔때마다 불러오게끔\r\n        // app.vue가 나타날때 데이터바인딩에 저장해놓고 프로필추가 or 다른프로필 선택할때\r\n        // 위에서 저장한 데이터바인딩에 덮어쓰기 하기 로그인데이터값을 app으로 보내기\r\n        // 근데 프로필 데이터바인딩 = db의 프로필데이터이니까 오케이\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\ndiv {\r\n  box-sizing: border-box;\r\n}\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.login-page {\r\n  position: absolute;\r\n  width: 1024px;\r\n  height: 100vh;\r\n  background-color: rgb(53, 74, 28);\r\n  z-index: 1000;\r\n}\r\n</style>\r\n"]}]}