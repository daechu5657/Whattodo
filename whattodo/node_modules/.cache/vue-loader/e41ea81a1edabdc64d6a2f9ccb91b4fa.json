axios';\r\nimport abc from './abc.json';\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 응답: {},\r\n      // 보낼거: { a: 'b' },\r\n      index: 0,\r\n      데이터: { index: '', title: '', daynight: '', time: '', order: '' },\r\n      텍스트: [],\r\n    };\r\n  },\r\n  methods: {\r\n    // more() {\r\n    //   axios.get('http://1cff-1-236-125-4.ngrok.io/a').then(response => {\r\n    //     console.log(response.data[0]);\r\n    //     this.응답 = response.data[0];\r\n    //   });\r\n    // },\r\n    // send() {\r\n    //   axios.post('/', this.보낼거).then(response => {\r\n    //     console.log(response.data);\r\n    //   });\r\n    // },\r\n    push() {\r\n      if (this.index >= 3) {\r\n        alert('박슈는 4개까지만..ㅎ');\r\n        console.log(this.텍스트);\r\n        console.log(this.데이터);\r\n        console.log(abc);\r\n      } else if (this.index == 0) {\r\n        this.텍스트.push({ value: '' });\r\n        this.데이터.index = this.index;\r\n        this.index++;\r\n      } else if (this.index == 1) {\r\n        this.텍스트.push({ value: '' });\r\n        this.index++;\r\n      } else if (this.index == 2) {\r\n        this.텍스트.push({ value: '' });\r\n        this.index++;\r\n      }\r\n      //반복문 쓰면 다 뽑아올수있음\r\n      // console.log(this.벨류);\r\n      // console.log(this.저장);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\ndiv {\r\n  box-sizing: border-box;\r\n}\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.modalwrap {\r\n  position: absolute;\r\n  width: 924px;\r\n  height: 80vh;\r\n  background-color: rgba(147, 70, 70, 0.9);\r\n  z-index: 100;\r\n}\r\n.modal-top {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 10vh;\r\n}\r\n.modal-content {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 60vh;\r\n}\r\n.modal-bottom {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 10vh;\r\n}\r\n</style>\r\n"]}]}