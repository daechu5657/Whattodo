(fisrt);\r\n    console.log(last);\r\n\r\n    console.log(firstday);\r\n    console.log(lastday);\r\n    //0~6 월~일\r\n    console.log(firstdate);\r\n    console.log(lastdate);\r\n\r\n    // this.lastdate1[4] = lastdate;\r\n\r\n    //firstday가 5임 그럼 +1해서 6만큼 앞에 공란이여야함\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.daygrid-top {\r\n  display: grid;\r\n  grid-template-columns: repeat(7, 1fr);\r\n  background-color: beige;\r\n}\r\n.daygird-main {\r\n  display: grid;\r\n  grid-template-columns: repeat(7, 1fr);\r\n}\r\n</style>\r\n"]}]}