/span>\r\n      <!-- 요론식인데 텍스트 없을때 undefind뜨면 this.alarm0 을 watch로 조건식걸어서? -->\r\n      <span>알람2:{{ alarm1 }}</span>\r\n      <span>알람3:{{ alarm2 }}</span>\r\n      <button @click=\"modal()\">추가</button>\r\n      <button @click=\"날짜테스트()\">테스트</button>\r\n    </section>\r\n    <Modal\r\n      v-if=\"modalOnOff == 1\"\r\n      :profileN=\"profileN\"\r\n      :modalOnOff=\"modalOnOff\"\r\n      @modalOnOffchange=\"modal()\"\r\n    />\r\n    <Calender @Cmonthday=\"Cmonthday\" v-if=\"CalenderOnOff == 1\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Modal from './Modal.vue';\r\nimport Calender from './Calender.vue';\r\nlet today = new Date();\r\nlet m = today.getMonth() + 1;\r\nlet d = today.getDate();\r\nexport default {\r\n  components: {\r\n    Modal: Modal,\r\n    Calender: Calender,\r\n  },\r\n  props: {\r\n    profileN: Number,\r\n    profile: Array,\r\n    Tmonth: String,\r\n    Tday: String,\r\n    dayOnOff: Number,\r\n    CalenderOnOff: Number,\r\n  },\r\n  data() {\r\n    return {\r\n      modalOnOff: 0,\r\n      month: '',\r\n      day: '',\r\n      Cmonth: '',\r\n      Cday: '',\r\n      alarm0: [],\r\n      alarm1: [],\r\n      alarm2: [],\r\n      notthing: 1,\r\n    };\r\n  },\r\n  methods: {\r\n    modal() {\r\n      if (this.modalOnOff == 0) {\r\n        this.modalOnOff = 1;\r\n      } else {\r\n        this.modalOnOff = 0;\r\n      }\r\n    },\r\n    날짜테스트() {\r\n      const a = this.profile[this.profileN].contents.find(\r\n        x => x.month == '4' && x.day == '19'\r\n      );\r\n      console.log(a);\r\n    },\r\n    // 처음은 현재날짜로 입력하고\r\n    // 만약 하단의 today를 클릭하면 day를 불러오고 현재날짜를  위 month day에 대입\r\n    // 만약 캘린더를 통해서 들어오면 day를 불러오고 emit(날짜)을 day로 줘서\r\n    // 위 month day에 대입할 수 있도록\r\n    Cmonthday(a) {\r\n      this.month = a[0];\r\n      this.day = a[1];\r\n      $emit('CalenderDayon');\r\n    },\r\n  },\r\n  watch: {\r\n    profile() {\r\n      if (this.Tmonth == m && this.Tday == d) {\r\n        this.month = m;\r\n        this.day = d;\r\n        // 그리고 날짜에 맞는 배열을 선택해야하니\r\n\r\n        const a = this.profile[this.profileN].contents.find(\r\n          x => x.month == this.month && x.day == this.day\r\n        );\r\n        // 그리고 a의 alarm부분을 this.alarm0 this.alarm1 this.alarm2 에 저장하는데\r\n        // alarm이 항상 있는건 아니니 조건식으로\r\n\r\n        if (a.alarmN.length == 3) {\r\n          this.alarm0 = a.alarmN[0];\r\n          this.alarm1 = a.alarmN[1];\r\n          this.alarm2 = a.alarmN[2];\r\n          this.notthing = 0;\r\n        } else if (a.alarmN.length == 2) {\r\n          this.alarm0 = a.alarmN[0];\r\n          this.alarm1 = a.alarmN[1];\r\n          this.notthing = 0;\r\n        } else if (a.alarmN.length == 1) {\r\n          this.alarm0 = a.alarmN[0];\r\n          this.notthing = 0;\r\n        } else {\r\n          this.notthing = 1;\r\n          // notthing=1이면 컨텐츠 부분에 '일정이 없습니다'라고 뜨게 v-if 로 할꺼임\r\n        }\r\n      } else {\r\n        // 캘린더에서 보낸숫자를 emit에 날짜 데이터 있으니까 그걸 this.Cmonth\r\n        // this.Cday로 저장후\r\n        this.month = this.Cmonth;\r\n        this.day = this.Cday;\r\n\r\n        // 그리고 a의 alarm부분을 this.alarm0 this.alarm1 this.alarm2 에 저장하는데\r\n        // alarm이 항상 있는건 아니니 조건식으로\r\n        // 위와동일\r\n\r\n        const a = this.profile[this.profileN].contents.find(\r\n          x => x.month == this.month && x.day == this.day\r\n        );\r\n        if (a.alarmN.length == 3) {\r\n          this.alarm0 = a.alarmN[0];\r\n          this.alarm1 = a.alarmN[1];\r\n          this.alarm2 = a.alarmN[2];\r\n          this.notthing = 0;\r\n        } else if (a.alarmN.length == 2) {\r\n          this.alarm0 = a.alarmN[0];\r\n          this.alarm1 = a.alarmN[1];\r\n          this.notthing = 0;\r\n        } else if (a.alarmN.length == 1) {\r\n          this.alarm0 = a.alarmN[0];\r\n          this.notthing = 0;\r\n        } else {\r\n          this.notthing = 1;\r\n          // notthing=1이면 컨텐츠 부분에 '일정이 없습니다'라고 뜨게 v-if 로 할꺼임\r\n        }\r\n      }\r\n    },\r\n  },\r\n  // created() {\r\n  //   this.month=m\r\n  //   this.day=d\r\n  // 처음에 현재날짜 설정\r\n  // },\r\n};\r\n</script>\r\n\r\n<style>\r\ndiv {\r\n  box-sizing: border-box;\r\n}\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.daywrap {\r\n  position: relative;\r\n  width: 1024px;\r\n  height: 90vh;\r\n}\r\n.day-top {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 10vh;\r\n  background-color: antiquewhite;\r\n}\r\n.day-notice {\r\n  position: relative;\r\n  display: inline-block;\r\n  width: 100px;\r\n  height: 80vh;\r\n  background-color: darkgoldenrod;\r\n}\r\n.day-contents {\r\n  position: relative;\r\n  display: inline-block;\r\n  width: 924px;\r\n  height: 80vh;\r\n  background-color: darkslategrey;\r\n}\r\n</style>\r\n"]}]}