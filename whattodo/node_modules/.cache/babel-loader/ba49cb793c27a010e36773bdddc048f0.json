        <button @click=\"del()\">삭제</button>\r\n      </div>\r\n      <div class=\"modal-bottom\">\r\n        <!-- <p>이름?{{ 응답.이름 }}</p>\r\n      <button @click=\"more()\">이름받기</button>\r\n      <button @click=\"send()\">보내기</button> -->\r\n        <button @click=\"$emit('modalOnOffchange')\">닫기</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nexport default {\r\n  data() {\r\n    return {\r\n      index: 0,\r\n      senddata: {\r\n        _id: '',\r\n        profileN: '',\r\n        month: '',\r\n        day: '',\r\n        alarmNumber: 0,\r\n        title: '',\r\n        time: '',\r\n        text: [],\r\n      },\r\n    };\r\n  },\r\n  props: {\r\n    profileN: Number,\r\n    modalOnOff: Number,\r\n    month: Number,\r\n    day: Number,\r\n    userid: String,\r\n    alarmindex: Number,\r\n  },\r\n  methods: {\r\n    // more() {\r\n    //   axios.get('http://1cff-1-236-125-4.ngrok.io/a').then(response => {\r\n    //     console.log(response.data[0]);\r\n    //     this.응답 = response.data[0];\r\n    //   });\r\n    // },\r\n    // send() {\r\n    //   axios.post('/', this.보낼거).then(response => {\r\n    //     console.log(response.data);\r\n    //   });\r\n    // },\r\n    push() {\r\n      if (this.index >= 3) {\r\n        alert('박슈는 4개까지만..ㅎ');\r\n        // console.log(this.텍스트);\r\n        // console.log(this.데이터);\r\n      } else if (this.index == 0) {\r\n        this.senddata.text.push({ text: '' });\r\n        // this.데이터.index = this.index;\r\n        this.index++;\r\n      } else if (this.index == 1) {\r\n        this.senddata.text.push({ text: '' });\r\n        this.index++;\r\n      } else if (this.index == 2) {\r\n        this.senddata.text.push({ text: '' });\r\n        this.index++;\r\n      }\r\n    },\r\n    save() {\r\n      this.senddata._id = this.userid;\r\n      this.senddata.profileN = this.profileN;\r\n      this.senddata.month = this.month;\r\n      this.senddata.day = this.day;\r\n      this.senddata.alarmNumber = this.alarmindex;\r\n      if (this.index == 0) {\r\n        alert('박슈만들어유');\r\n      } else if (this.index == 1 && this.senddata.text[0].text == '') {\r\n        alert('내용입력해쥬');\r\n      } else if (\r\n        this.index == 2 &&\r\n        (this.senddata.text[0].text == 0 || this.senddata.text[1].text == '')\r\n      ) {\r\n        alert('내용입력해쥬');\r\n      } else if (\r\n        this.index == 3 &&\r\n        (this.senddata.text[0].text == 0 ||\r\n          this.senddata.text[1].text == '' ||\r\n          this.senddata.text[2].text == '')\r\n      ) {\r\n        alert('내용입력해쥬');\r\n      } else if (this.senddata.title == '') {\r\n        alert('제목입력해쥬');\r\n      } else if (this.senddata.time == '') {\r\n        alert('시간입력해쥬');\r\n      } else {\r\n        axios.post('/ModalUpdate', this.senddata).then(응답 => {\r\n          console.log(응답);\r\n        });\r\n      }\r\n    },\r\n    del() {\r\n      if (this.index <= 0) {\r\n        alert('삭제할꺼업슈');\r\n      } else this.index--;\r\n      this.senddata.text.pop();\r\n    },\r\n  },\r\n  watch: {},\r\n};\r\n</script>\r\n\r\n<style>\r\ndiv {\r\n  box-sizing: border-box;\r\n}\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n.modal-outside {\r\n  position: absolute;\r\n  width: 1024px;\r\n  height: 90vh;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  top: 0;\r\n}\r\n.modalwrap {\r\n  position: absolute;\r\n  width: 512px;\r\n  height: 70vh;\r\n  background-color: rgba(147, 70, 70, 0.9);\r\n  z-index: 100;\r\n  left: 256px;\r\n  top: 10vh;\r\n}\r\n.modal-top {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 10vh;\r\n}\r\n.modal-content {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 50vh;\r\n}\r\n.modal-bottom {\r\n  position: relative;\r\n  width: inherit;\r\n  height: 10vh;\r\n}\r\n</style>\r\n"]}]}