AAeU,CAAC,CAACX,IAAF,CAAO,CAAP,EAAUC,OAAzB;AACD,KAPM;AAQPa,wBARO,gCAQcH,CARd,EAQiB;AACtB,WAAKV,OAAL,GAAeU,CAAC,CAACX,IAAF,CAAO,CAAP,EAAUC,OAAzB;AACD,KAVM;AAWPc,kBAXO,4BAWU;AACf,UAAI,KAAKZ,QAAL,IAAiB,CAArB,EAAwB;AACtB,aAAKA,QAAL,GAAgB,CAAhB;AACF,OAFA,MAEO;AACL,aAAKA,QAAL,GAAgB,CAAhB;AACF;AACD,KAjBM;AAkBPa,cAlBO,wBAkBM;AACX,WAAKR,aAAL,GAAqB,CAArB;AACA,WAAKD,OAAL,GAAe,CAAf;AACD,KArBM;AAsBPU,iBAtBO,2BAsBS;AACd,WAAKT,aAAL,GAAqB,CAArB;AACA,WAAKD,OAAL,GAAe,CAAf;AACD;AAzBM,GAnBI;AA8CbW,OAAK,EAAE;AACLjB,WADK,mBACGD,IADH,EACS;AACZ,WAAKK,WAAL,CAAiB,CAAjB,EAAoBA,WAApB,GAAkCL,IAAI,CAAC,CAAD,CAAJ,CAAQK,WAA1C;AACA,WAAKA,WAAL,CAAiB,CAAjB,EAAoBA,WAApB,GAAkCL,IAAI,CAAC,CAAD,CAAJ,CAAQK,WAA1C;AACA,WAAKC,YAAL,CAAkB,CAAlB,EAAqBA,YAArB,GAAoCN,IAAI,CAAC,CAAD,CAAJ,CAAQM,YAA5C;AACA,WAAKA,YAAL,CAAkB,CAAlB,EAAqBA,YAArB,GAAoCN,IAAI,CAAC,CAAD,CAAJ,CAAQM,YAA5C;AACD,KANI,CAOL;AACA;;AARK;AA9CM,CAAf","names":["Day","Login","ProfileModal","name","components","data","profile","userid","profileN","profileOnOff","profilename","profileimage","TodayOn","CalenderOnOff","methods","getid","a","_id","updateprofile","updateprofilebymodal","profileNchange","clicktoday","clickCalender","watch"],"sourceRoot":"","sources":["C:\\Users\\q\\Desktop\\프토플리오\\Whattodo\\whattodo\\src\\App.vue"],"sourcesContent":["<template>\n  <div class=\"wrap\">\n    <div class=\"center\">\n      <div class=\"centerwrap\">\n        <Login @getid=\"getid\" />\n        <div class=\"main\">\n          <Day\n            :profileN=\"profileN\"\n            :profile=\"profile\"\n            :TodayOn=\"TodayOn\"\n            @CalenderOff=\"CalenderOnOff = 0\"\n            :CalenderOnOff=\"CalenderOnOff\"\n            @clickCalender=\"clickCalender\"\n            :userid=\"userid\"\n            @updateprofile=\"updateprofile\"\n            @updateprofilebymodal=\"updateprofilebymodal\"\n          />\n        </div>\n        <div class=\"footer\">\n          <div class=\"footer-contents\">\n            <span @click=\"clicktoday\">Today</span>\n            <span @click=\"clickCalender\">Calendar</span>\n            <span>Settings</span>\n            <div class=\"profile\">\n              <div class=\"profile-1\" @mouseover=\"profileOnOff = 'on'\">\n                {{ profilename[profileN].profilename }}\n                {{ profileimage[profileN].profileimage }}\n              </div>\n              <ProfileModal\n                :profileOnOff=\"profileOnOff\"\n                :profilename=\"profilename\"\n                :profileimage=\"profileimage\"\n                :profileN=\"profileN\"\n                @profileNchange=\"profileNchange\"\n                @profileOnOff=\"profileOnOff = 'off'\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Day from './components/Day.vue';\nimport Login from './components/Login.vue';\nimport ProfileModal from './components/ProfileModal.vue';\n\n// import axios from 'axios';\nexport default {\n  name: 'App',\n  components: {\n    Day: Day,\n    Login: Login,\n    ProfileModal: ProfileModal,\n  },\n  data() {\n    return {\n      profile: [],\n      userid: '',\n      profileN: 0,\n      profileOnOff: 'off',\n      profilename: [{ profilename: '' }, { profilename: '' }],\n      profileimage: [{ profileimage: '' }, { profileimage: '' }],\n      TodayOn: 0,\n      CalenderOnOff: 0,\n    };\n  },\n  methods: {\n    getid(a) {\n      this.userid = a.data[0]._id;\n      this.profile = a.data[0].profile;\n    },\n    updateprofile(a) {\n      this.profile = a.data[0].profile;\n    },\n    updateprofilebymodal(a) {\n      this.profile = a.data[0].profile;\n    },\n    profileNchange() {\n      if (this.profileN == 0) {\n        this.profileN = 1;\n      } else {\n        this.profileN = 0;\n      }\n    },\n    clicktoday() {\n      this.CalenderOnOff = 0;\n      this.TodayOn = 1;\n    },\n    clickCalender() {\n      this.CalenderOnOff = 1;\n      this.TodayOn = 0;\n    },\n  },\n  watch: {\n    profile(data) {\n      this.profilename[0].profilename = data[0].profilename;\n      this.profilename[1].profilename = data[1].profilename;\n      this.profileimage[0].profileimage = data[0].profileimage;\n      this.profileimage[1].profileimage = data[1].profileimage;\n    },\n    // //결국 변동은 모달에서 나타나니까 모달에서 생성에 emit달아서 axios get하도록\n    // 하면 계속 업데이트 되서 쭉쭉 props할듯\n  },\n};\n</script>\n\n<style>\ndiv {\n  box-sizing: border-box;\n}\nbody {\n  margin: 0;\n  padding: 0;\n}\n.wrap {\n  position: relative;\n  width: 100vw;\n  height: 100vh;\n  background-color: #242424;\n}\n.center {\n  position: relative;\n  width: 1024px;\n  height: 100%;\n  margin: 0 auto;\n}\n.centerwrap {\n  position: relative;\n  width: 100%;\n  height: 100%;\n  background: linear-gradient(#253739, #555546);\n  /* background-image: url('./assets/background.png');\n  background-repeat: no-repeat;\n  background-size: 1024px 100vh; */\n}\n.main {\n  position: relative;\n  width: 1024px;\n  height: 90vh;\n  background-color: rgba(0, 0, 0, 0.45);\n}\n.footer {\n  position: relative;\n  width: 1024px;\n  height: 10vh;\n  background-color: rgba(0, 0, 0, 0.45);\n}\n\n.footer-contents {\n  position: relative;\n  width: 1000px;\n  height: 9vh;\n  left: 12px;\n  padding: 0 150px;\n  border-radius: 30px;\n  background: rgba(0, 0, 0, 0.3);\n  box-shadow: 0px 0px 7px rgba(0, 0, 0, 0.3);\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n}\n.footer-contents > span {\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin: 2vh 0;\n  color: rgba(255, 255, 255, 0.8);\n  transition: 0.3s ease;\n  font-size: 18px;\n  font-weight: bold;\n  background-color: rgba(66, 63, 64, 0);\n  border-radius: 15px;\n}\n.footer-contents > span:hover {\n  background-color: rgba(54, 54, 54, 0.6);\n  border-radius: 30px;\n  transition: 0.3s ease;\n  border: 0;\n}\n</style>\n"]}]}